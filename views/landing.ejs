<html>
    <head>
        <title>Custom Trim View Page</title>
        <style>
            li{
              max-height:100px;
              margin:0;
              overflow:auto;
              padding:0;
            }
        </style>
    </head>
    
    <body>
        <h1>data view page</h1>
        
        
        <ul>
            <% data.forEach(function(datum) { %>
                <% if(datum.entry.includes("static")){ %>
                    <li><a href="<%= datum.entry %>">Re-converted PDF file</a></li>
                <% }else{ %>
                    <li><%= datum.entry %></li>
                <% } %>
            <% }); %>
        </ul>
    </body>

</html>